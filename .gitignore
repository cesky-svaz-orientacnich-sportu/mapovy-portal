# Ignore bundler config
/.bundle
vendor/bundle

# Ignore the default SQLite database and SQL dumps.
/db/*.sqlite3
/db/*.sql

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

/public/data

/public/system
/spec/reports
.DS_Store
